<military>

    <prototype>
        <name>mil_enrichment</name>
        <config>
            <Enrichment>
                <max_feed_inventory>8000</max_feed_inventory>
                <feed_commods>
                    <val>uf6</val>
                </feed_commods>
                <feed_commod_prefs>
                    <val>1</val>
                </feed_commod_prefs>
                
                <feed_recipe>r_uox</feed_recipe>

                <swu_capacity>8333</swu_capacity>
                <product_commod>mil_fiss</product_commod>

                <tails_assay>0.003</tails_assay>
                <tails_commod>mil_u_dep</tails_commod>
                
                <tails_change_times><val>122</val></tails_change_times>
                <tails_change_commods><val>mil_uf6</val></tails_change_commods>
                <tails_change_assay><val>0.0071</val></tails_change_assay>
                
                <feed_change_times><val>122</val></feed_change_times>
                <feed_change_commods>
                    <val>
                        <val>civ_leu</val>
                    </val>
                </feed_change_commods>
                <prefs_change_prefs>
                    <val>
                        <val>1</val>
                </val>
                </prefs_change_prefs>
            
            </Enrichment>
        </config>
    </prototype>

    <prototype>
        <name>mil_str_u_dep</name>
        <config>
            <Storage>
                <in_commods>
                    <val>mil_u_dep</val>
                </in_commods>
                <out_commods>
                    <val>mil_u_dep_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>mil_uox_fabrication</name>
        <config>
            <Storage>
                <max_inv_size>30000</max_inv_size>
                <in_recipe>r_mil_uox</in_recipe>
                <in_commods>
                    <val>uf6</val>
                </in_commods>
                <in_commod_prefs>
                    <val>1</val>
                </in_commod_prefs>
                <out_commods>
                    <val>mil_uox</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>mil_lwr</name>
        <lifetime>960</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>  
                    <val>r_mil_uox</val>
                </fuel_inrecipes>
                <fuel_outrecipes> 
                    <val>r_mil_uox_spent</val>
                </fuel_outrecipes>
                <fuel_incommods>  
                    <val>mil_uox</val>
                </fuel_incommods>
                <fuel_outcommods> 
                    <val>mil_uox_spent</val>
                </fuel_outcommods>
                <fuel_prefs>
                    <val>1</val>
                </fuel_prefs>


                <cycle_time>4</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>14000</assem_size>
                <n_assem_core>1</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>0.15</power_cap>
            </Reactor>
        </config>
    </prototype>
    
    <prototype>
        <name>mil_str_mox_spent</name>
        <config>
            <Storage>
                <in_commods>
                    <val>mil_mox_spent</val>
                </in_commods>
                <out_commods>
                    <val>mil_mox_spent_str</val>
                </out_commods>
                <residence_time>60</residence_time>
            </Storage>
        </config>
    </prototype>
    
    <prototype>
        <name>reprocessing</name>
        <config>
            <Separations>
                <feed_commod_prefs>
                    <val>2.0</val>
                    <val>1.0</val>
                </feed_commod_prefs>
                <feed_commods>
                    <val>mil_uox_spent</val>
                    <val>uox_spent_str</val>
                </feed_commods>
                <feedbuf_size>30000000000</feedbuf_size>
                <leftover_commod>waste</leftover_commod>
                <streams>
                    <item>
                        <commod>mil_fiss</commod>
                        <info>
                            <buf_size>3000000000</buf_size>
                            <efficiencies>
                                <item>
                                    <comp>Pu</comp> <eff>.95</eff>
                                </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>
                <throughput>1e100</throughput>
            </Separations>
        </config>
    </prototype>
    
    <prototype>
        <name>mil_str_fiss</name>
        <config>
            <Storage>
                <max_inv_size>1e100</max_inv_size>
                <in_recipe>r_mil_heu</in_recipe>
                <in_commods>
                    <val>mil_fiss</val>
                </in_commods>
                <in_commod_prefs>
                    <val>10</val>
                </in_commod_prefs>
                <out_commods>
                    <val>mil_heu</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

</military>
