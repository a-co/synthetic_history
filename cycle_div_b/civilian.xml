<civilian>

    <prototype>
        <name>civ_enrichment</name>
        <config>
            <Enrichment>
                <max_feed_inventory>100000</max_feed_inventory>
                <feed_commods>
                    <val>uf6</val>
                    <val>mil_uf6</val>
                </feed_commods>
                <feed_commod_prefs>
                    <val>1</val>
                    <val>2</val>
                </feed_commod_prefs>
                <feed_recipe>r_natl_u</feed_recipe>

                <swu_capacity>30000</swu_capacity>
                <product_commod>civ_leu</product_commod>

                <tails_assay>0.003</tails_assay>
                <tails_commod>u_dep</tails_commod>
            </Enrichment>
        </config>
    </prototype>

    <prototype>
        <name>civ_str_u_dep</name>
        <config>
            <Storage>
                <in_commods>
                    <val>u_dep</val>
                </in_commods>
                <out_commods>
                    <val>u_dep_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>civ_fabrication</name>
        <config>
            <Storage>
                <max_inv_size>30000</max_inv_size>
                <in_recipe>r_uox</in_recipe>
                <in_commods>
                    <val>civ_leu</val>
                </in_commods>
                <in_commod_prefs>
                    <val>1000</val>
                </in_commod_prefs>
                <out_commods>
                    <val>uox</val>
                </out_commods>
                <residence_time>1</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>civ_lwr</name>
        <lifetime>960</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>  
                    <val>r_uox</val>
                </fuel_inrecipes>
                <fuel_outrecipes> 
                    <val>r_uox_spent</val>
                </fuel_outrecipes>
                <fuel_incommods>  
                    <val>uox</val>
                </fuel_incommods>
                <fuel_outcommods> <val>uox_spent</val></fuel_outcommods>

                <cycle_time>18</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>29565</assem_size>
                <n_assem_core>3</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>900</power_cap>
            </Reactor>
        </config>
    </prototype>

    <prototype>
        <name>civ_str_uox_spent</name>
        <config>
            <Storage>
                <in_commods>
                    <val>uox_spent</val>
                </in_commods>
                <out_commods>
                    <val>uox_spent_str</val>
                </out_commods>
                <residence_time>60</residence_time>
            </Storage>
        </config>
    </prototype>

</civilian>
